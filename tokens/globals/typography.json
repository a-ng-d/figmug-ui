{
  "font": {
    "$type": "typography",
    "size": {
      "xxxsmall": {
        "$type": "dimension",
        "$value": { "value": 9, "unit": "px" }
      },
      "xxsmall": {
        "$type": "dimension",
        "$value": { "value": 10, "unit": "px" }
      },
      "xsmall": {
        "$type": "dimension",
        "$value": { "value": 11, "unit": "px" }
      },
      "small": {
        "$type": "dimension",
        "$value": { "value": 12, "unit": "px" }
      },
      "medium": {
        "$type": "dimension",
        "$value": { "value": 13, "unit": "px" }
      },
      "large": {
        "$type": "dimension",
        "$value": { "value": 14, "unit": "px" }
      }
    },
    "weight": {
      "light": {
        "$type": "fontWeight",
        "$value": 300
      },
      "normal": {
        "$type": "fontWeight",
        "$value": 400
      },
      "medium": {
        "$type": "fontWeight",
        "$value": 500
      },
      "semi": {
        "$type": "fontWeight",
        "$value": 600
      },
      "bold": {
        "$type": "fontWeight",
        "$value": 700
      }
    },
    "lineHeight": {
      "default": {
        "$type": "dimension",
        "$value": { "value": 16, "unit": "px" }
      },
      "large": {
        "$type": "dimension",
        "$value": { "value": 20, "unit": "px" }
      }
    },
    "letterSpacing": {
      "neg": {
        "xlarge": {
          "$type": "dimension",
          "$value": "-0.12px"
        },
        "large": {
          "$type": "dimension",
          "$value": "-0.06px"
        },
        "medium": {
          "$type": "dimension",
          "$value": "-0.03px"
        },
        "small": {
          "$type": "dimension",
          "$value": "-0.012px"
        }
      },
      "pos": {
        "small": {
          "$type": "dimension",
          "$value": "0.012px"
        },
        "medium": {
          "$type": "dimension",
          "$value": "0.03px"
        },
        "large": {
          "$type": "dimension",
          "$value": "0.06px"
        },
        "xlarge": {
          "$type": "dimension",
          "$value": "0.12px"
        }
      }
    }
  }
}
