{
  "text": {
    "fontFamily": {
      "$value": ["Work Sans", "sans-serif"],
      "$type": "fontFamily"
    },
    "color": {
      "primary": {
        "$value": "{penpot.color.foreground.primary}",
        "$type": "color"
      },
      "secondary": {
        "$value": "{penpot.color.foreground.secondary}",
        "$type": "color"
      },
      "tertiary": {
        "$value": "{penpot.color.foreground.disabled}",
        "$type": "color"
      },
      "inverse": {
        "$value": "{penpot.color.background.primary}",
        "$type": "color"
      }
    },
    "fontWeight": {
      "medium": {
        "$value": "{font.weight.medium}",
        "$type": "fontWeight"
      },
      "bold": {
        "$value": "{font.weight.bold}",
        "$type": "fontWeight"
      }
    },
    "default": {
      "$type": "typography",
      "$value": {
        "$fontFamily": "{text.fontFamily}",
        "fontSize": "{font.size.small}",
        "fontWeight": "{font.weight.medium}",
        "lineHeight": "{font.lineHeight.default}",
        "letterSpacing": "{font.letterSpacing.pos.large}"
      }
    },
    "small": {
      "$type": "typography",
      "$value": {
        "$fontFamily": "{text.fontFamily}",
        "fontSize": "{font.size.xsmall}",
        "fontWeight": "{font.weight.medium}",
        "lineHeight": "{font.lineHeight.default}",
        "letterSpacing": "{font.letterSpacing.pos.xlarge}"
      }
    },
    "large": {
      "$type": "typography",
      "$value": {
        "$fontFamily": "{text.fontFamily}",
        "fontSize": "{font.size.medium}",
        "fontWeight": "{font.weight.medium}",
        "lineHeight": "{font.lineHeight.large}",
        "letterSpacing": "{font.letterSpacing.pos.medium}"
      }
    },
    "xlarge": {
      "$type": "typography",
      "$value": {
        "$fontFamily": "{text.fontFamily}",
        "fontSize": "{font.size.large}",
        "fontWeight": "{font.weight.medium}",
        "lineHeight": "{font.lineHeight.large}",
        "letterSpacing": "{font.letterSpacing.pos.small}"
      }
    },
    "link": {
      "color": {
        "$value": "{penpot.color.accent.primary.default}",
        "$type": "color"
      },
      "decoration": {
        "$value": "none",
        "$type": "string"
      },
      "borderFocus": {
        "$value": "none",
        "$type": "string"
      },
      "decorationFocus": {
        "$value": "underline",
        "$type": "string"
      }
    },
    "label": {
      "$type": "typography",
      "$value": {
        "$fontFamily": "{text.fontFamily}",
        "fontSize": "{font.size.xsmall}",
        "fontWeight": "{font.weight.normal}",
        "lineHeight": "{font.lineHeight.default}",
        "letterSpacing": "{font.letterSpacing.pos.small}"
      },
      "color": {
        "$value": "{penpot.color.foreground.secondary}",
        "$type": "color"
      },
      "height": {
        "$value": "{size.pos.medium}",
        "$type": "dimension"
      },
      "padding": {
        "top": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "right": {
          "$value": "{size.pos.xxxsmall}",
          "$type": "dimension"
        },
        "bottom": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "left": {
          "$value": "{size.pos.xxsmall}",
          "$type": "dimension"
        }
      },
      "gap": {
        "$value": "{size.pos.xxxsmall}",
        "$type": "dimension"
      }
    }
  }
}
