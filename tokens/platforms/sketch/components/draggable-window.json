{
  "draggableWindow": {
    "width": {
      "$value": {
        "value": 320,
        "unit": "px"
      },
      "$type": "dimension"
    },
    "radius": {
      "$value": "{border.radius.xxxlarge}",
      "$type": "dimension"
    },
    "shadow": {
      "$value": "{elevation.500ModalWindow}",
      "$type": "shadow"
    },
    "background": {
      "color": {
        "$value": "{sketch.color.background.primary}",
        "$type": "color"
      }
    },
    "border": {
      "color": {
        "$value": "{sketch.color.border.primary}",
        "$type": "color"
      },
      "width": {
        "$value": "{size.pos.unit}",
        "$type": "dimension"
      },
      "offset": {
        "$value": "{size.neg.unit}",
        "$type": "dimension"
      }
    },
    "header": {
      "border": {
        "width": {
          "$value": "{size.pos.unit}",
          "$type": "dimension"
        },
        "color": {
          "$value": "{sketch.color.border.disabled}",
          "$type": "color"
        }
      }
    },
    "content": {
      "padding": {
        "top": {
          "$value": "{size.pos.xxsmall}",
          "$type": "dimension"
        },
        "right": {
          "$value": "{size.pos.xxsmall}",
          "$type": "dimension"
        },
        "bottom": {
          "$value": "{size.pos.xxsmall}",
          "$type": "dimension"
        },
        "left": {
          "$value": "{size.pos.xxsmall}",
          "$type": "dimension"
        }
      },
      "gap": {
        "$value": "{size.pos.xxxsmall}",
        "$type": "dimension"
      }
    }
  }
}
