{
  "accordion": {
    "padding": {
      "vertical": {
        "$value": "{size.pos.xxxsmall}",
        "$type": "dimension"
      },
      "horizontal": {
        "$value": "{size.null}",
        "$type": "dimension"
      }
    },
    "border": {
      "width": {
        "$value": "{size.pos.unit}",
        "$type": "dimension"
      },
      "color": {
        "$value": "{figma.color.border.disabled}",
        "$type": "color"
      }
    },
    "row": {
      "gap": {
        "$value": "{size.pos.xsmall}",
        "$type": "dimension"
      },
      "height": {
        "$value": "{size.pos.medium}",
        "$type": "dimension"
      },
      "padding": {
        "vertical": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "horizontal": {
          "$value": "{size.pos.xsmall}",
          "$type": "dimension"
        }
      }
    },
    "text": {
      "color": {
        "default": {
          "$value": "{figma.color.text.secondary}",
          "$type": "color"
        },
        "hover": {
          "$value": "{figma.color.text}",
          "$type": "color"
        },
        "expanded": {
          "$value": "{figma.color.text}",
          "$type": "color"
        },
        "disabled": {
          "$value": "{figma.color.text.disabled}",
          "$type": "color"
        }
      }
    },
    "icon": {
      "color": {
        "default": {
          "$value": "{figma.color.icon.secondary}",
          "$type": "color"
        },
        "hover": {
          "$value": "{figma.color.icon}",
          "$type": "color"
        },
        "expanded": {
          "$value": "{figma.color.icon}",
          "$type": "color"
        },
        "disabled": {
          "$value": "{figma.color.icon.disabled}",
          "$type": "color"
        }
      }
    }
  }
}
