{
  "draggable-item": {
    "template": {
      "rows": {
        "$value": "{size.pos.medium}",
        "$type": "dimension"
      },
      "columns": {
        "$value": "repeat(76, 1fr)",
        "$type": "string"
      }
    },
    "padding": {
      "top": {
        "$value": "{size.pos.xunit}",
        "$type": "dimension"
      },
      "right": {
        "$value": "{size.pos.xsmall}",
        "$type": "dimension"
      },
      "bottom": {
        "$value": "{size.pos.xunit}",
        "$type": "dimension"
      },
      "left": {
        "$value": "{size.pos.xsmall}",
        "$type": "dimension"
      }
    },
    "background": {
      "color": {
        "default": {
          "$value": "{figma.color.bg}",
          "$type": "color"
        },
        "selected": {
          "$value": "{figma.color.bg.selected}",
          "$type": "color"
        },
        "pressed": {
          "$value": "{figma.color.bg.pressed}",
          "$type": "color"
        }
      }
    },
    "gap": {
      "$value": "{size.pos.xxsmall}",
      "$type": "dimension"
    },
    "ghost": {
      "color": {
        "$value": "{figma.color.border.selected}",
        "$type": "color"
      },
      "height": {
        "$value": "{size.pos.xunit}",
        "$type": "dimension"
      },
      "inset": {
        "$value": "{size.neg.unit}",
        "$type": "dimension"
      }
    },
    "param": {
      "width": {
        "compact": {
          "$value": {
            "value": 112,
            "unit": "px"
          },
          "$type": "dimension"
        },
        "square": {
          "$value": "{size.pos.medium}",
          "$type": "dimension"
        }
      }
    }
  }
}
