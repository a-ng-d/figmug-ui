{
  "shadow": {
    "$type": "shadow",
    "$description": "A comprehensive set of shadow values to ensure consistent shadow effects across all supported platforms. These values can be used for box-shadow, text-shadow, and other shadow needs.",
    "$extensions": {
      "com.figma.scopes": ["EFFECT_FLOAT", "OPACITY"]
    },
    "hud": {
      "$value": [
        {
          "offsetX": "{size.null}",
          "offsetY": "{size.pos.xxxsmall}",
          "blur": "{size.xsmall}",
          "spread": "{size.null}",
          "color": {
            "$type": "color",
            "colorSpace": "srgb",
            "components": [0, 0, 0],
            "alpha": "{alpha.200}"
          }
        },
        {
          "offsetX": "{size.null}",
          "offsetY": "{size.pos.xunit}",
          "blur": "{size.xxsmall}",
          "spread": "{size.null}",
          "color": {
            "$type": "color",
            "colorSpace": "srgb",
            "components": [0, 0, 0],
            "alpha": "{alpha.150}"
          }
        }
      ]
    },
    "floatingWindow": {
