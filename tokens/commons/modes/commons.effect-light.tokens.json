{
  "shadow": {
    "$type": "boxShadow",
    "$description": "A comprehensive set of shadow values to ensure consistent shadow effects across all supported platforms. These values can be used for box-shadow, text-shadow, and other shadow needs.",
    "$extensions": {
      "com.figma.scopes": ["EFFECT_FLOAT", "OPACITY"]
    },
    "hud": {
      "$value": [
        {
          "x": "{size.null}",
          "y": "{size.pos.xxxsmall}",
          "blur": "{size.xsmall}",
          "spread": "{size.null}",
          "color": "{shadow.color.black.alpha200}"
        },
        {
          "x": "{size.null}",
          "y": "{size.pos.xunit}",
          "blur": "{size.xxsmall}",
          "spread": "{size.null}",
          "color": "{shadow.color.black.alpha150}"
        }
